    "description": "Integrate Google Services into Symfony 2 app.",
    "keywords": ["google", "user", "contacts"],
    "homepage": "http://bitgandtter.github.com",
    "license": "MIT",
    "authors": [
        {
            "name":"Margaret Staples",
            "email":"mstaples@aesopgames.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
