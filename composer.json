        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": ">=2.1",
        "symfony/security-bundle": ">=2.1",
        "symfony/twig-bundle": ">=2.1",
        "mstaples/google-api": "dev-master"
